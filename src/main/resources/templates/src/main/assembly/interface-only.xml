<assembly>
	<id>interface-only</id>
	<formats>
		<format>jar</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<fileSets>
		<fileSet>
			<directory>${r"${project.build.directory}"}/classes</directory>
			<outputDirectory>/</outputDirectory>
			<fileMode>0644</fileMode>
			<includes>
				<include>${basePackageNamePath}service/*</include>
			</includes>
			<excludes>
				<exclude>${basePackageNamePath}service/impl</exclude>
			</excludes>
		</fileSet>
	</fileSets>
</assembly>